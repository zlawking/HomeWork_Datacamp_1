    "hour = 0\n",
    "min = 0\n",
    "while True :\n",
    "   hour = int(input(\"จำนวนชั่วโมงที่จอด :\"))\n",
    "   min = int(input(\"จำนวนนาทีที่จอด :\"))\n",
    "   if min <= 60 :\n",
    "      break\n",
    "if min >= 15 :\n",
    "   hour = hour+1\n",
    "else:\n",
    "   hour = hour - 1\n",
    "cost = (hour*300 )\n",
    "print(\"ค่าที่จอดรถ {} บาท\".format(cost))"
